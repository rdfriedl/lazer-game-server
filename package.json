  "devDependencies": {
    "babel-cli": "^6.26.0",
    "cross-env": "^5.1.4",
    "husky": "^0.14.3",
    "nodemon": "^1.17.2",
    "prettier": "^1.11.1",
    "pretty-quick": "^1.4.1",
    "rimraf": "^2.6.2"
  },
  "prettier": {
    "useTabs": true,
    "semi": true,
    "printWidth": 120
